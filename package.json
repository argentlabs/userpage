{
  "name": "userpage",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@xstate/react": "^1.5.1",
    "beautiful-react-hooks": "^0.35.0",
    "bnc-onboard": "^1.32.0",
    "ethers": "^5.4.3",
    "jotai": "^1.3.2",
    "js-cookie": "^3.0.1",
    "lodash.chunk": "^4.2.0",
    "lodash.debounce": "^4.0.8",
    "lottie-react": "^2.1.0",
    "qr-code-styling": "^1.6.0-rc.1",
    "react": "^17.0.2",
    "react-copy-to-clipboard": "^5.0.3",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "styled-components": "^5.3.0",
    "styled-normalize": "^8.0.7",
    "styled-reset": "^4.3.4",
    "styled-tools": "^1.7.2",
    "swr": "^1.0.0",
    "typescript": "^4.1.2",
    "url-join": "^4.0.1",
    "use-resize-observer": "^7.0.1",
    "web-vitals": "^1.0.1",
    "xstate": "^4.23.1"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "pretest": "npm run lint",
    "test": "yarn test:all",
    "test:watch": "yarn test:all:watch",
    "test:all": "yarn test:all:watch --watchAll=false",
    "test:all:watch": "react-scripts test --passWithNoTests --testPathIgnorePatterns \"__mocks__\" --testPathIgnorePatterns \"^e2e\"",
    "test:unit": "yarn test:all --testNamePattern=\"^\\[unit\\]\"",
    "test:int": "yarn test:all --testNamePattern=\"^\\[int\\]\"",
    "e2e": "yarn e2e:chromium",
    "pree2e:all": "playwright install",
    "e2e:all": "run-p --race serve e2e:playwright:all",
    "pree2e:chromium": "playwright install chromium",
    "e2e:chromium": "run-p --race serve e2e:playwright:chromium",
    "pree2e:firefox": "playwright install firefox",
    "e2e:firefox": "run-p --race serve e2e:playwright:firefox",
    "pree2e:webkit": "playwright install webkit",
    "e2e:webkit": "run-p --race serve e2e:playwright:webkit",
    "e2e:playwright": "sleep 5 && playwright test e2e",
    "e2e:playwright:chromium": "yarn e2e:playwright --browser=chromium",
    "e2e:playwright:firefox": "yarn e2e:playwright --browser=firefox",
    "e2e:playwright:webkit": "yarn e2e:playwright --browser=webkit",
    "e2e:playwright:all": "yarn e2e:playwright --browser=all",
    "posttest": "npm run format",
    "eject": "react-scripts eject",
    "format": "prettier --loglevel warn --ignore-path .gitignore --write \"**/*.{js,jsx,ts,tsx,css,md,yml,yaml,json}\"",
    "lint": "eslint . --cache --fix --ignore-path .gitignore",
    "prepare": "husky install",
    "typechain": "typechain --target ethers-v5 --out-dir ./src/generated './src/abis/**/*.json'",
    "postinstall": "yarn typechain",
    "serve": "serve -p 9999 --no-port-switching -s build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@ethereum-waffle/jest": "^3.3.0",
    "@playwright/test": "^1.13.1",
    "@typechain/ethers-v5": "^7.0.1",
    "@types/js-cookie": "^2.2.7",
    "@types/lodash.chunk": "^4.2.6",
    "@types/lodash.debounce": "^4.0.6",
    "@types/react-copy-to-clipboard": "^5.0.1",
    "@types/react-helmet": "^6.1.2",
    "@types/react-router-dom": "^5.1.8",
    "@types/styled-components": "^5.1.11",
    "@types/url-join": "^4.0.1",
    "eslint-config-prettier": "^8.3.0",
    "ethereum-waffle": "^3.4.0",
    "husky": ">=6",
    "import-sort-style-module": "^6.0.0",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": ">=10",
    "npm-run-all": "^4.1.5",
    "prettier": ">=2",
    "prettier-plugin-import-sort": "^0.0.7",
    "serve": "^12.0.0",
    "source-map-explorer": "^2.5.2",
    "type-fest": "^1.3.0",
    "typechain": "^5.1.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*.{js,jsx,ts,tsx,css,md,yml,yaml,json}": "prettier --write"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  }
}
